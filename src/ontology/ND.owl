    <!-- http://purl.obolibrary.org/obo/ND#ND_diagnostic_criterion -->

    <ObjectProperty rdf:about="&obo;ND#ND_diagnostic_criterion">
        <rdfs:label>has diagnostic criterion</rdfs:label>
        <obo:IAO_0000115>A has diagnostic criterion B = B is either a necessary, sufficient, or partially sufficient condition for diagnosing a patient with A* according to the diagnostic guideline A.</obo:IAO_0000115>
        <obo:IAO_0000117>Alexander P. Cox</obo:IAO_0000117>
        <rdfs:domain rdf:resource="&obo;ND_0001100"/>
        <rdfs:range rdf:resource="&obo;OGMS_0000014"/>
    </ObjectProperty>
    


    <!-- http://purl.obolibrary.org/obo/ND#ND_major_diagnostic_criterion -->

    <ObjectProperty rdf:about="&obo;ND#ND_major_diagnostic_criterion">
        <rdfs:label>has major diagnostic criterion</rdfs:label>
        <obo:IAO_0000117>Alexander P. Cox</obo:IAO_0000117>
        <obo:IAO_0000115>A has major diagnostic criterion B = B is a necessary or sufficient condition for diagnosing a patient with A* according to the diagnostic guideline A.</obo:IAO_0000115>
        <obo:IAO_0000118>has primary diagnostic criterion</obo:IAO_0000118>
        <rdfs:domain rdf:resource="&obo;ND_0001100"/>
        <rdfs:range rdf:resource="&obo;OGMS_0000014"/>
        <rdfs:subPropertyOf rdf:resource="&obo;ND#ND_diagnostic_criterion"/>
    </ObjectProperty>
    


    <!-- http://purl.obolibrary.org/obo/ND#ND_minor_diagnostic_criterion -->

    <ObjectProperty rdf:about="&obo;ND#ND_minor_diagnostic_criterion">
        <rdfs:label>has minor diagnostic criterion</rdfs:label>
        <obo:IAO_0000115>A has minor diagnostic criterion B = B is a non-necessary but partially sufficient condition for diagnosing a patient with A* according to the diagnostic guideline A.</obo:IAO_0000115>
        <obo:IAO_0000118>has secondary diagnostic criterion</obo:IAO_0000118>
        <obo:IAO_0000117>Alexander P. Cox</obo:IAO_0000117>
        <obo:IAO_0000118>has supporting diagnostic criterion</obo:IAO_0000118>
        <rdfs:domain rdf:resource="&obo;ND_0001100"/>
        <rdfs:range rdf:resource="&obo;OGMS_0000014"/>
        <rdfs:subPropertyOf rdf:resource="&obo;ND#ND_diagnostic_criterion"/>
    </ObjectProperty>
    


    <!-- http://purl.obolibrary.org/obo/ND#ND_satisfies -->

    <ObjectProperty rdf:about="&obo;ND#ND_satisfies">
        <rdfs:label>satisfies</rdfs:label>
        <obo:IAO_0000117>Alexander P. Cox</obo:IAO_0000117>
        <obo:IAO_0000115>A satisfies B = A is a clinical finding that matches diagnostic criterion A* which is part of diagnostic guideline B</obo:IAO_0000115>
        <rdfs:range rdf:resource="&obo;ND_0001100"/>
        <rdfs:domain rdf:resource="&obo;OGMS_0000014"/>
    </ObjectProperty>
    


    <!-- http://purl.obolibrary.org/obo/ND#definitely_indicates -->

    <ObjectProperty rdf:about="&obo;ND#definitely_indicates">
        <rdfs:label>indicates definite presence of</rdfs:label>
        <obo:IAO_0000115>A indicates definite presence of B = A is a diagnosis of a patient that hypothesizes with a very high degree of certainty that the disease, disorder, syndrome, or nothing abnormal of type B inheres in the patient.</obo:IAO_0000115>
        <obo:IAO_0000117>Alexander P. Cox</obo:IAO_0000117>
        <rdfs:subPropertyOf rdf:resource="&obo;ND#indicates"/>
    </ObjectProperty>
    


    <!-- http://purl.obolibrary.org/obo/ND#indicates -->

    <ObjectProperty rdf:about="&obo;ND#indicates">
        <rdfs:label>indicates presence of</rdfs:label>
        <obo:IAO_0000115>A indicates presence of B = A is a diagnosis of a patient that hypothesizes that the disease, disorder, syndrome, or nothing abnormal of type B inheres in the patient.</obo:IAO_0000115>
        <obo:IAO_0000117>Alexander P. Cox</obo:IAO_0000117>
        <rdfs:domain rdf:resource="&obo;OGMS_0000073"/>
        <rdfs:subPropertyOf rdf:resource="&obo;ND#is_about"/>
        <rdfs:range>
            <Class>
                <unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&obo;OGMS_0000031"/>
