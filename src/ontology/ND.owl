    <!-- http://purl.obolibrary.org/obo/ND#lacks -->

    <ObjectProperty rdf:about="&obo;ND#lacks">
        <rdfs:label>lacks clinical finding</rdfs:label>
        <obo:IAO_0000117>Alexander P. Cox</obo:IAO_0000117>
        <obo:IAO_0000115>A lacks clinical finding B = B is a clinical finding, A is a diagnostic guideline, and the presence of B must be ruled out in order to conclude a diagnostic process with a diagnosis of A*, where A* is the diagnosis associated with A.</obo:IAO_0000115>
        <rdfs:domain rdf:resource="&obo;ND_0001100"/>
        <rdfs:range rdf:resource="&obo;OGMS_0000014"/>
    </ObjectProperty>
    


